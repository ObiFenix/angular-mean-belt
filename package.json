{
  "name": "eRR-backend",
  "version": "1.0.0",
  "description": "A MEAN (Mongo Express Angular Node) Single Page App",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "client-install": "yarn install --prefix front-end",
    "client": "yarn start --prefix",
    "server": "nodemon app.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "node",
    "npm",
    "yarn",
    "nodemon"
  ],
  "author": "ObiFenix",
  "license": "MIT",
  "dependencies": {
    "@ionic/app-scripts": "^3.2.2",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "global": "^4.3.2",
    "mongoose": "^5.2.4",
    "nodemon": "^1.18.9"
  }
}
